{
  "name": "nickolasburr/ocache",
  "description": "Virtual object cache for PHP.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Nickolas Burr",
      "email": "nickolasburr@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "psr/simple-cache": "^2.0||^3.0",
    "symfony/var-exporter": "~6.2"
  },
  "autoload": {
    "files": [
        "src/functions.php",
        "src/Exception/functions.php",
        "src/Stream/functions.php"
